{Spin2_v49}

OBJ m : "Spin2_v49_step_child"

CON struct mpoint = m.point, struct myPoint(x,y), struct square(myPoint tlCrnr, myPoint brCrnr), struct polygon(myPoint crnr[6])

PUB go(m.point pa, myPoint pb) : myPoint retA, m.point retB  | m.point la, mpoint lb, polygon lc

  la.x := 1
  la.y := 2
  lb.x := 3
  lb.y := 4

  lc[4] := 5,6
  retA := pa
  retB := pb

  debug(sdec(la,lb))
